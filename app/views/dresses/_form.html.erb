<%= simple_form_for(@dress) do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">
    <%= f.input :category %>
  </div>
  <div class="form-inputs">
    <%= f.input :title %>
  </div>
  <div class="form-inputs">
    <%= f.input :description %>
  </div>
  <div class="form-inputs">
    <%= f.input :price %>
  </div>
  <div class="form-inputs">
    <%= f.input :size%>
  </div>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>